				<string>function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

function alfredArgs(args) {
  return JSON.stringify({ alfredworkflow: { arg: args } })
}

function run(argv) {
  const dataFolder = $(envVar("alfred_workflow_data")).stringByDeletingLastPathComponent.js
  const cacheFolder = $(envVar("alfred_workflow_cache")).stringByDeletingLastPathComponent.js
  const workflowBundleID = Application("System Events").propertyListFiles
    .byName(`${argv[0]}/info.plist`)
    .contents.value().bundleid

  if (workflowBundleID.length === 0) return

  return alfredArgs([`${dataFolder}/${workflowBundleID}`, `${cacheFolder}/${workflowBundleID}`])
}</string>
